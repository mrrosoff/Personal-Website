{
    "name": "personal-website",
    "version": "4.3.0",
    "description": "My Personal Website",
    "scripts": {
        "clean": "rm -rf coverage dist",
        "check-format": "prettier --check \"src/**/*.ts*\"",
        "format": "prettier --write \"src/**/*.ts*\"",
        "types": "tsc --noEmit",
        "start": "vite",
        "build-website": "npm run types && vite build",
        "build-cdk": "npm run types && NODE_ENV=production node esbuild.mjs",
        "bootstrap": "cdk bootstrap",
        "deploy": "cdk deploy --require-approval=never --all"
    },
    "author": "Max Rosoff",
    "license": "MIT",
    "dependencies": {
        "@aws-sdk/client-dynamodb": "^3.799.0",
        "@aws-sdk/client-s3": "^3.802.0",
        "@aws-sdk/credential-providers": "^3.799.0",
        "@aws-sdk/lib-dynamodb": "^3.799.0",
        "@aws-sdk/lib-storage": "^3.802.0",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@mui/icons-material": "^7.0.2",
        "@mui/lab": "^7.0.0-beta.11",
        "@mui/material": "^7.0.2",
        "aws-cdk-lib": "^2.194.0",
        "axios": "^1.9.0",
        "constructs": "^10.4.2",
        "email-validator": "^2.0.4",
        "get-options": "^1.2.0",
        "luxon": "^3.6.1",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-router-dom": "^7.5.3",
        "resend": "^4.5.0"
    },
    "devDependencies": {
        "@tsconfig/node22": "^22.0.1",
        "@types/aws-lambda": "^8.10.149",
        "@types/luxon": "^3.6.2",
        "@types/node": "^22.15.3",
        "@types/react": "^19.1.2",
        "@types/react-dom": "^19.1.3",
        "@vitejs/plugin-react": "^4.4.1",
        "aws-cdk": "^2.1013.0",
        "dotenv": "^16.5.0",
        "esbuild": "^0.25.3",
        "prettier": "^3.5.3",
        "sass": "^1.87.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3",
        "vite": "^6.3.4"
    }
}
